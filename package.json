{
  "name": "my-react",
  "version": "1.0.0",
  "description": "template for react apps",
  "main": "index.js",
  "engines": {
    "node": "16.20.0",
    "npm": "8.19.4"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "analyze:prod": "webpack --config ./webpack.config.prod.js --env.mode=analyze --env.env=production",
    "analyze:dev": "webpack --config ./webpack.config.dev.js --env.mode=analyze --env.env=development",
    "dev-server": "webpack-dev-server --config ./webpack.config.dev.js --mode development --hot --port 3000",
    "build-client:ssr": "webpack --config ./webpack.config.client.js --env.mode=SSR --env.env=production",
    "build-server:ssr": "webpack --config ./webpack.config.server.js --env.mode=SSR --env.env=production",
    "build:ssr": "npm run clean && npm run build-client:ssr && npm run build-server:ssr && npm run start:prod:ssr",
    "build-client:spa": "webpack --config ./webpack.config.client.js --env mode=SPA --env env=production",
    "build-server:spa": "webpack --config ./webpack.config.server.js --env mode=SPA --env env=production",
    "build:spa": "npm run clean && npm run build-client:spa && npm run build-server:spa && npm run start:prod:spa",
    "start:dev": "cross-env NODE_MODE=SPA NODE_ENV=development GOOGLE_APPLICATION_CREDENTIALS=./service_account.json BABEL_DISABLE_CACHE=1 nodemon ./entry.js",
    "start:debug": "cross-env NODE_MODE=SPA NODE_ENV=development BABEL_DISABLE_CACHE=1 nodemon --inspect ./entry.js",
    "start:prod:ssr": "cross-env NODE_MODE=SSR NODE_ENV=production node ./entry.js",
    "start:prod:spa": "cross-env NODE_MODE=SPA NODE_ENV=production GOOGLE_APPLICATION_CREDENTIALS=./service_account.json node ./entry.js",
    "start": "node ./entry.js",
    "heroku-postbuild": "npm run build-client:spa && npm run build-server:spa",
    "dev": "npm run clean && npm run start:dev",
    "dev:debug": "npm run clean && npm run start:debug",
    "test:all": "cross-env NODE_ENV=test jest --config=jest.config.js",
    "test:models": "cross-env NODE_ENV=test jest model.spec --config=jest.config.js --detectOpenHandles",
    "test:endpoints": "cross-env NODE_ENV=test jest integration.spec --config=jest.config.js --detectOpenHandles",
    "test:react": "cross-env NODE_ENV=test jest /__tests__/.* --config=jest.config.js --detectOpenHandles",
    "test:e2e": "cross-env NODE_ENV=test"
  },
  "author": "Nick Savickas",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.21.5",
    "@fortawesome/fontawesome-svg-core": "1.2.26",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "5.5.0",
    "@fortawesome/react-fontawesome": "0.1.8",
    "@google-cloud/secret-manager": "^4.2.2",
    "@google-cloud/storage": "^6.10.1",
    "@loadable/component": "~5.0.1",
    "@loadable/server": "~5.1.0",
    "async-redis": "^1.1.7",
    "axios": "0.18.1",
    "body-parser": "1.18.2",
    "bootstrap": "3.3.7",
    "composable-middleware": "0.3.0",
    "compression": "1.7.4",
    "connected-react-router": "5.0.1",
    "cookie-parser": "1.4.4",
    "core-js": "^3.30.2",
    "cors": "^2.8.5",
    "cross-env": "5.1.6",
    "crypto-js": "3.1.9-1",
    "errorhandler": "1.5.1",
    "express": "4.16.4",
    "express-fileupload": "1.0.0",
    "express-jwt": "5.3.1",
    "express-session": "1.15.6",
