    "lodash": "4.17.15",
    "lusca": "1.6.1",
    "method-override": "3.0.0",
    "moment": "^2.26.0",
    "mongoose": "^7.2.0",
    "morgan": "1.9.1",
    "multer": "1.4.2",
    "passport": "0.4.1",
